{
  "name": "rata-project",
  "version": "1.1.2",
  "description": "Basic school exercises",
  "private": true,
  "scripts": {
    "start": "yarn run gen-ver && react-scripts start",
    "build": "yarn run gen-ver && yarn test && react-scripts build && yarn build:sw && cp src/version.json build && yarn run tar",
    "build:sw": "rm -f build/static/js/*.js.map build/service-worker.js build/precache-manifest.*.js && node ./build-sw.js",
    "serve": "serve -s build",
    "test": "yarn run lint # && react-scripts test",
    "gen-ver": "echo \"export const version = '${npm_package_version}';\" > src/version.js && echo \"{\n  \\\"version\\\": \\\"${npm_package_version}\\\"\n}\" > src/version.json",
    "lint": "eslint src ./*.js",
    "tar": "mkdir -p dist && rm -f dist/*.tgz && COPYFILE_DISABLE=1 tar cfz dist/${npm_package_name}-${npm_package_version}.tgz build && echo '\n*** DONE ***' && ls -1 dist"
  },
  "dependencies": {
    "lz-string": "^1.4.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-icons": "^3.7.0",
    "react-redux": "7.1.0",
    "react-router5": "7.1.0",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-router5": "7.0.2",
    "redux-saga": "1.0.5",
    "reduxsauce": "^1.0.0",
    "router5": "7.0.2",
    "router5-plugin-browser": "^7.0.2",
    "router5-transition-path": "7.0.1",
    "serve": "^11.2.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "eslint-config-airbnb-base": "^13.1.0",
    "workbox-build": "^4.3.1"
  },
  "author": "Roman Kaspar <roman@salsitasoft.com> (https://salsitasoft.com/)",
  "license": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
